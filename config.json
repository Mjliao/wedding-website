{
  "baseURL": "https://alohalaurenmichael.netlify.app/",
  "languageCode": "en-us",
  "title": "Lauren & Michael's Wedding",
  "show_comments": false,
  "disableKinds": [
    "sitemap",
    "RSS"
  ],
  "markup": {
    "defaultMarkdownHandler": "goldmark",
    "goldmark": {
      "extensions": {
        "definitionList": true,
        "footnote": true,
        "linkify": true,
        "linkifyProtocol": "https",
        "strikethrough": true,
        "table": true,
        "taskList": true,
        "typographer": true
      },
      "parser": {
        "attribute": {
          "block": false,
          "title": true
        },
        "autoHeadingID": true,
        "autoHeadingIDType": "github"
      },
      "renderer": {
        "hardWraps": false,
        "unsafe": true,
        "xhtml": false
      }
    }
  },
  "params": {
    "background_image_2": "https://mikeliaophotography.s3.us-west-1.amazonaws.com/travel/70.jpg",
    "background_image_4": "https://mikeliaophotography.s3.us-west-1.amazonaws.com/travel/75.jpg",
    "background_image_3": "https://mikeliaophotography.s3.us-west-1.amazonaws.com/people/144.jpg",
    "background_image_1": "https://mikeliaophotography.s3.us-west-1.amazonaws.com/people/168.jpg",
    "background_image_5": "https://mikeliaophotography.s3.us-west-1.amazonaws.com/travel/91.jpg"
  }
